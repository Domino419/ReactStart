const IterationSample2 = () => {
    const name = ['눈사람', '얼음', '눈', '바람'] ; 
    
    const nameList  = name.map((name, index) => <li key={index}> {name} </li>) ; 
    console.log("::::::::::::::::::::::::: IterationSample.js ")
    
    return (   
        <ul>
            {nameList}
        </ul>
    ) ; 
} ; 

export default IterationSample2 ; 

